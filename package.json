{
  "name": "code-generator-var",
  "version": "0.1.0",
  "author": "rocyuan",
  "description": "VAR管理平台代码生成器",
  "main": "electron/app.electrom.js",
  "private": true,
  "scripts": {
    "dev": "node script/dev.js",
    "start": "node script/dev_start.js",
    "ele:dev": "nodemon --exec electron-forge start --watch ./ --ext .html,.js,.jsx,.css,.vue,.scss,.json",
    "ele:start": "electron-forge start",
    "build:package": "rimraf out && npm run web:build && electron-forge package",
    "build:make": "rimraf out && npm run web:build && electron-forge make",
    "package": "rimraf out && electron-forge package",
    "make": "rimraf out && electron-forge make",
    "web:dev": "vue-cli-service serve",
    "web:serve": "vue-cli-service serve",
    "web:build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "core-js": "^3.8.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "element-plus": "^2.3.14",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "roc-utils": "^0.2.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-wix": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^25.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "lint-staged": "^11.1.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.4.1",
    "rimraf": "^5.0.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
